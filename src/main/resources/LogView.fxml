      <VBox spacing="10.0">
         <children>
            <HBox alignment="TOP_CENTER" spacing="10.0">
               <children>
                  <HBox alignment="CENTER" spacing="1.0">
                     <children>
                        <TextField fx:id="currentMarkTextField" alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="31.0" prefWidth="110.0" text="1" />
